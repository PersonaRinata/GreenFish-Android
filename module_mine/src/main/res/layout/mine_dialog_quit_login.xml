<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="276dp"
        android:layout_height="143dp"
        android:background="@drawable/mine_shape_confirm_out_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="39dp"
            android:text="确定退出登录？"
            android:textColor="@color/fish_font_level4_color"
            android:textSize="14.74sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/mine_dialog_quit_login_tv_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取消"
            android:textColor="@color/mine_cancel_text_color"
            android:background="@drawable/mine_shape_cancel_bg"
            android:textSize="13.89sp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="23dp"
            android:paddingTop="4.71dp"
            android:paddingBottom="3.28dp"
            android:paddingStart="18.84dp"
            android:paddingEnd="19.78dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/mine_dialog_quit_login_tv_confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="确定"
            android:textColor="@color/white"
            android:background="@drawable/mine_shape_confirm_bg"
            android:textSize="13.89sp"
            android:paddingTop="4.71dp"
            android:paddingBottom="3.28dp"
            android:paddingStart="18.84dp"
            android:paddingEnd="19.78dp"
            android:layout_marginEnd="48dp"
            android:layout_marginTop="23dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>